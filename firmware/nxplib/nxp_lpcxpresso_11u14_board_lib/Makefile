include ../../defaultc.mk

INCLUDES	+= -I../lpc_chip_11uxx_lib/inc/

all: $(SLIB)

$(SLIB): ${OBJS}
	mkdir -p $(LIBDIR)
	$(AR) -r -o $@ $(OBJS)
	mv $(SLIB) $(LIBDIR)

clean:
	rm -f $(OBJS) $(SLIB) $(LIBDIR)/$(SLIB)
